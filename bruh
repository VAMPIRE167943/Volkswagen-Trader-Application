        break_me_onee_chan.setVisible(true);
        this.setVisible(false);
    }//GEN-LAST:event_ahegaoActionPerformed

    private void crush_me_onee_chanActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_crush_me_onee_chanActionPerformed
        if(nyash.isSelected() == true){
            try {
                PrintWriter thicc = new PrintWriter("I like turtles.txt");
                thicc.println("Personal Information");
                thicc.println("----------------------------------------------");
                thicc.println("Fullnames: " + domme.getText() + " " + feet.getText());
                thicc.println("ID Number: " + dominatrix.getText());
                thicc.println("Cell Number: " + toes.getText());
                if(futa.getSelectedItem() == "Male"){
                    thicc.println("Gender: Male");
                }else if(futa.getSelectedItem() == "Female"){
                    thicc.println("Gender: Female");
                }else{
                    thicc.println("Gender: Other");
                }
                thicc.println("Ocuupation: " + cough.getText());
                thicc.println("Home Address: " + choke.getText());
                thicc.println("Work Address: " + swallow.getText());
                thicc.println("Gross Pay: R" + slurp.getText());
                thicc.println("Net Pay: R" + suck.getText());
                thicc.println("Valid License: Yes");
                if(uncensored.getSelectedItem() == "Polo Vivo"){
                    thicc.println("Model: Polo Vivo");
                }else if(uncensored.getSelectedItem() == "Polo"){
                    thicc.println("Model: Polo");
                }else if(uncensored.getSelectedItem() == "T-Cross"){
                    thicc.println("Model: T-Cross");
                }else{
                    thicc.println("Model: T-Roc");
                }
                thicc.println("\nNext of kin Information.");
                thicc.println("----------------------------------------------");
                thicc.println("Fullnames: " + censored.getText() + " " + maidenless.getText());
                thicc.println("ID Number: " + skrrt.getText());
                thicc.println("ID Number: " + yeet.getText());
                thicc.close();
                JOptionPane.showMessageDialog(null, "Check your file Senpai UwU");
            } catch (FileNotFoundException ex) {
                
            }
        }
    }//GEN-LAST:event_crush_me_onee_chanActionPerformed

