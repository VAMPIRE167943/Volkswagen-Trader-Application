    private void bussyActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_bussyActionPerformed
        if(bussy.getSelectedItem() == "Polo Vivo"){
            thighs.setText("R 3 166");
            thongs.setText("R 237 400");
            bigini.setText("5.5%");
            briefs.setText("0%");
            cat_girls.setText("R 94 960");
            dog_girls.setText("72");
            hentie.setText("R 300 276");
            ImageIcon isekai = new ImageIcon("C:\\Users\\jason\\JAVA522FA2\\src\\java522fa2\\img\\comfortline.png");
            cheeky.setIcon(isekai);
        }else if(bussy.getSelectedItem() == "Polo"){
            thighs.setText("R 3 404");
            thongs.setText("R 224 300");
            bigini.setText("7.59%");
            briefs.setText("0%");
            cat_girls.setText("R 118 879");
            dog_girls.setText("48");
            hentie.setText("R 282 249");
            ImageIcon saucy = new ImageIcon("C:\\Users\\jason\\JAVA522FA2\\src\\java522fa2\\img\\poloTsi.png");
            cheeky.setIcon(saucy);
        }else if(bussy.getSelectedIndex() == 3){
            thighs.setText("R 4 450");
            thongs.setText("R 352 300");
            bigini.setText("5.57.59%");
            briefs.setText("10.58%");
            cat_girls.setText("R 183 196");
            dog_girls.setText("48");
            hentie.setText("R 393 796");
            ImageIcon booba = new ImageIcon("C:\\Users\\jason\\JAVA522FA2\\src\\java522fa2\\img\\tcross.png");
            cheeky.setIcon(booba);
        }else if(bussy.getSelectedIndex() == 4){
            thighs.setText("R 5 950");
            thongs.setText("R 494 300");
            bigini.setText("7.59%");
            briefs.setText("10.49%");
            cat_girls.setText("R 271 865");
            dog_girls.setText("48");
            hentie.setText("R 557 465");
            ImageIcon uwu = new ImageIcon("C:\\Users\\jason\\JAVA522FA2\\src\\java522fa2\\img\\troc.png");
            cheeky.setIcon(uwu);
        }else{
            thighs.setText(" ");
            thongs.setText(" ");
            bigini.setText(" ");
            briefs.setText(" ");
            cat_girls.setText(" ");
            dog_girls.setText(" ");
            hentie.setText(" ");
            ImageIcon owo = new ImageIcon(" ");
            cheeky.setIcon(owo);
        }
    }//GEN-LAST:event_bussyActionPerformed

